{
    "extends": ["eslint:recommended", "plugin: import/errors", "plugin:react/recommended","plugin:jsx-a11y/recommended", "prettier", "prettier/react"],
    "plugins": ["react", "import", "jsx-a11y"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceTypes": "module",
        "ecmaFeatures": {
            "jsx": true,
            "no-console": 1
        }
    },
    "rules": {
        "react/prop-types": 0,
        "no-console": 1
    },
    "env": {
        "es6": true, 
        "browser": true,
        "node": true
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}